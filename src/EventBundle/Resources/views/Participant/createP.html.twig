<!doctype html>
<html lang="en">

<head>
    <title>Schooli</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="{{ asset ('https://fonts.googleapis.com/css?family=DM+Sans:300,400,700|Indie+Flower')}}" rel="stylesheet">


    <link rel="stylesheet" href="{{ asset ('fonts/icomoon/style.css')}}">

    <link rel="stylesheet" href="{{ asset ('css/bootstrap.min.css')}}">
    <link rel="stylesheet" href="{{ asset ('css/bootstrap-datepicker.css')}}">
    <link rel="stylesheet" href="{{ asset ('css/jquery.fancybox.min.css')}}">
    <link rel="stylesheet" href="{{ asset ('css/owl.carousel.min.css')}}">
    <link rel="stylesheet" href="{{ asset ('css/owl.theme.default.min.css')}}">
    <link rel="stylesheet" href="{{ asset ('fonts/flaticon/font/flaticon.css')}}">
    <link rel="stylesheet" href="{{ asset ('css/aos.css')}}">

    <!-- MAIN CSS -->
    <link rel="stylesheet" href="{{ asset ('css/style.css')}}">

</head>
<body>
<!---------------------------------------->
<br>
{% block container %}
    {{ form_start(part) }}

    <p><a  href="{{ path ('event_affiche')}}" class="btn btn-dark">BACK</a></p>


    <div style="background-color: peachpuff" class="container">
        <br>
        <h2 style="margin-left: 250px" class="col-sm-8"> </h2>
        <br>
        <div class="form-group row">
            <label for="input-27" class="col-sm-2 col-form-label">First Name</label>
            <div class="col-sm-10">
                {{ form_widget(part.nom,{'attr':{'class':'form-control'}}) }}
            </div>
        </div>

        <div class="form-group row">
            <label for="input-28" class="col-sm-2 col-form-label">Last Name</label>
            <div class="col-sm-10">
                {{ form_widget(part.prenom,{'attr':{'class':'form-control'}}) }}
            </div>
        </div>

        <div class="form-group row">
            <label for="input-29" class="col-sm-2 col-form-label">Phone Number</label>
            <div class="col-sm-10">
                {{ form_widget(part.num,{'attr':{'class':'form-control'}}) }}
            </div>
        </div>

        <div class="form-group row">
            <label for="input-29" class="col-sm-2 col-form-label">Event</label>
            <div class="col-sm-10">
                {{ form_widget(part.event,{'attr':{'class':'form-control'}}) }}
            </div>
        </div>
        <br>
        <div class="container-contact100-form-btn" style="margin-left: 500px" >
						<span>
                             <button type="submit" class="btn btn-sm btn-primary btn-create"> Save</button>
                        </span>
        </div>

<br>
    </div>
    {{ form_end(part) }}

{% endblock %}
<!---------------------------------------->


<script src="{{ asset ('js/jquery-3.3.1.min.js')}}"></script>
<script src="{{ asset ('js/jquery-migrate-3.0.0.js')}}"></script>
<script src="{{ asset ('js/popper.min.js')}}"></script>
<script src="{{ asset ('js/bootstrap.min.js')}}"></script>
<script src="{{ asset ('js/owl.carousel.min.js')}}"></script>
<script src="{{ asset ('js/jquery.sticky.js')}}"></script>
<script src="{{ asset ('js/jquery.waypoints.min.js')}}"></script>
<script src="{{ asset ('js/jquery.animateNumber.min.js')}}"></script>
<script src="{{ asset ('js/jquery.fancybox.min.js')}}"></script>
<script src="{{ asset ('js/jquery.stellar.min.js')}}"></script>
<script src="{{ asset ('js/jquery.easing.1.3.js')}}"></script>
<script src="{{ asset ('js/bootstrap-datepicker.min.js')}}"></script>
<script src="{{ asset ('js/aos.js')}}"></script>

<script src="{{ asset ('js/main.js')}}"></script>

</body>

</html>